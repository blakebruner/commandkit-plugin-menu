{
  "name": "commandkit-plugin-menu",
  "description": "Container menu plugin for CommandKit",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "commandkit",
    "componentsv2",
    "plugin",
    "menu",
    "pagination",
    "discordjs"
  ],
  "author": "Blake Bruner",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "bun run build",
    "pack:local": "bun run build && npm pack --pack-destination ../_packs"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "commandkit": "^1.2.0-rc.11",
    "discord.js": "^14.22.1"
  },
  "dependencies": {
    "@types/lodash": "^4.17.20",
    "lodash": "^4.17.21"
  }
}
